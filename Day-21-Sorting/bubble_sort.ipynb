   "source": [
    "arr = list(map(int, input(\"Enter numbers: \").split()))\n",
    "\n",
    "n = len(arr)\n",
    "for i in range(n):\n",
    "    for j in range(0, n - i - 1):\n",
    "        if arr[j] > arr[j + 1]:\n",
    "            arr[j], arr[j + 1] = arr[j + 1], arr[j]\n",
    "\n",
    "print(\"Sorted array:\", arr)"
   ]
