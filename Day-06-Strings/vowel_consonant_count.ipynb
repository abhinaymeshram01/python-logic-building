   ],
   "source": [
    "s = input(\"Enter string: \").lower()\n",
    "vowels = \"aeiou\"\n",
    "v = c = 0\n",
    "\n",
    "for ch in s:\n",
    "    if ch.isalpha():\n",
    "        if ch in vowels:\n",
    "            v += 1\n",
    "        else:\n",
    "            c += 1\n",
    "\n",
    "print(\"Vowels:\", v)\n",
    "print(\"Consonants:\", c)"
   ]
