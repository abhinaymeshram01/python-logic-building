   ],
   "source": [
    "arr = list(map(int, input(\"Enter elements: \").split()))\n",
    "n = len(arr)\n",
    "\n",
    "for i in range(n):\n",
    "    min_idx = i\n",
    "    for j in range(i + 1, n):\n",
    "        if arr[j] < arr[min_idx]:\n",
    "            min_idx = j\n",
    "\n",
    "    arr[i], arr[min_idx] = arr[min_idx], arr[i]\n",
    "\n",
    "print(\"Sorted list:\", arr)"
   ]
