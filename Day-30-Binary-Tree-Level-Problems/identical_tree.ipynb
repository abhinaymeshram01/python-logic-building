   "source": [
    "def is_identical(p, q):\n",
    "    if not p and not q:\n",
    "        return True\n",
    "\n",
    "    if not p or not q:\n",
    "        return False\n",
    "\n",
    "    return (\n",
    "        p.data == q.data and\n",
    "        is_identical(p.left, q.left) and\n",
    "        is_identical(p.right, q.right)\n",
    "    )"
   ]
