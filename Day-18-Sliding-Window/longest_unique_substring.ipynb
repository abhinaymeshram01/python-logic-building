   "source": [
    "s = input(\"Enter string: \")\n",
    "seen = set()\n",
    "left = 0\n",
    "max_len = 0\n",
    "\n",
    "for right in range(len(s)):\n",
    "    while s[right] in seen:\n",
    "        seen.remove(s[left])\n",
    "        left += 1\n",
    "\n",
    "    seen.add(s[right])\n",
    "    max_len = max(max_len, right - left + 1)\n",
    "\n",
    "print(\"Longest length:\", max_len)"
   ]
