   "source": [
    "arr = list(map(int, input(\"Enter numbers: \").split()))\n",
    "k = int(input(\"Enter sum: \"))\n",
    "\n",
    "prefix_sum = 0\n",
    "count = 0\n",
    "seen = {0: 1}\n",
    "\n",
    "for num in arr:\n",
    "    prefix_sum += num\n",
    "    count += seen.get(prefix_sum - k, 0)\n",
    "    seen[prefix_sum] = seen.get(prefix_sum, 0) + 1\n",
    "\n",
    "print(\"Count:\", count)"
   ]
