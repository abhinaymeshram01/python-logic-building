   "source": [
    "arr = list(map(int, input(\"Enter numbers: \").split()))\n",
    "\n",
    "max_sum = curr_sum = arr[0]\n",
    "\n",
    "for num in arr[1:]:\n",
    "    curr_sum = max(num, curr_sum + num)\n",
    "    max_sum = max(max_sum, curr_sum)\n",
    "\n",
    "print(\"Maximum subarray sum:\", max_sum)"
   ]
