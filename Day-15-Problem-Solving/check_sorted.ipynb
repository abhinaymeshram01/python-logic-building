   "source": [
    "arr = list(map(int, input(\"Enter numbers: \").split()))\n",
    "is_sorted = True\n",
    "\n",
    "for i in range(len(arr) - 1):\n",
    "    if arr[i] > arr[i + 1]:\n",
    "        is_sorted = False\n",
    "        break\n",
    "\n",
    "print(\"Sorted\" if is_sorted else \"Not Sorted\")"
   ]
