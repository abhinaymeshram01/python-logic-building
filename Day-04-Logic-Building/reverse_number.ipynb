   "source": [
    "num = int(input(\"Enter number: \"))\n",
    "rev = 0\n",
    "\n",
    "while num > 0:\n",
    "    rev = rev * 10 + num % 10\n",
    "    num //= 10\n",
    "\n",
    "print(\"Reversed number:\", rev)"
   ]
