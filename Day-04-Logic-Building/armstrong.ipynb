      "Armstrong Number\n"
     ]
    }
   ],
   "source": [
    "num = int(input(\"Enter number: \"))\n",
    "temp = num\n",
    "sum_val = 0\n",
    "digits = len(str(num))\n",
    "\n",
    "while temp > 0:\n",
    "    digit = temp % 10\n",
    "    sum_val += digit ** digits\n",
    "    temp //= 10\n",
    "\n",
    "if sum_val == num:\n",
    "    print(\"Armstrong Number\")\n",
    "else:\n",
    "    print(\"Not Armstrong\")"
   ]
