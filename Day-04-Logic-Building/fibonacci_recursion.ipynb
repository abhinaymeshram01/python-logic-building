   "source": [
    "def fib(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    return fib(n-1) + fib(n-2)\n",
    "\n",
    "terms = int(input(\"Enter terms: \"))\n",
    "for i in range(terms):\n",
    "    print(fib(i), end=\" \")"
   ]
