    "s = input(\"Enter string: \")\n",
    "freq = {}\n",
    "\n",
    "for ch in s:\n",
    "    freq[ch] = freq.get(ch, 0) + 1\n",
    "\n",
    "for ch in s:\n",
    "    if freq[ch] == 1:\n",
    "        print(\"First non-repeating:\", ch)\n",
    "        break\n",
    "else:\n",
    "    print(\"No non-repeating character\")"
