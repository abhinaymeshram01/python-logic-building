   "source": [
    "nums = list(map(int, input(\"Enter numbers: \").split()))\n",
    "target = int(input(\"Enter target: \"))\n",
    "\n",
    "seen = {}\n",
    "\n",
    "for i, num in enumerate(nums):\n",
    "    rem = target - num\n",
    "    if rem in seen:\n",
    "        print(\"Indices:\", seen[rem], i)\n",
    "        break\n",
    "    seen[num] = i"
   ]
