   "source": [
    "def array_sum(arr, n):\n",
    "    if n == 0:\n",
    "        return 0\n",
    "    return arr[n - 1] + array_sum(arr, n - 1)\n",
    "\n",
    "arr = list(map(int, input(\"Enter numbers: \").split()))\n",
    "print(\"Sum:\", array_sum(arr, len(arr)))"
   ]
