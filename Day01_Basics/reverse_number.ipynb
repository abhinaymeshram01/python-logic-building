   "source": [
    "# Problem:\n",
    "## Reverse a given number"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "3b3e1dfa-3de1-4157-a41e-ec2ecc02b556",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter a number:  34\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Reversed number: 43\n"
     ]
    }
   ],
   "source": [
    "number = int(input(\"Enter a number: \"))\n",
    "reverse_num = 0\n",
    "while number > 0:\n",
    "    digit = number % 10\n",
    "    reverse_num = reverse_num * 10 + digit\n",
    "    number = number // 10\n",
    "print(\"Reversed number:\", reverse_num)"
   ]
  },
