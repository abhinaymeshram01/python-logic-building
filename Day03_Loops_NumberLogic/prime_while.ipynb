   "source": [
    "num = int(input(\"Enter number: \"))\n",
    "i = 2\n",
    "is_prime = True\n",
    "\n",
    "if num <= 1:\n",
    "    is_prime = False\n",
    "else:\n",
    "    while i <= int(num**0.5):\n",
    "        if num % i == 0:\n",
    "            is_prime = False\n",
    "            break\n",
    "        i += 1\n",
    "\n",
    "print(\"Prime\" if is_prime else \"Not Prime\")"
   ]
