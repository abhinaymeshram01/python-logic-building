   "source": [
    "arr = list(map(int, input(\"Enter elements: \").split()))\n",
    "even = odd = 0\n",
    "\n",
    "for num in arr:\n",
    "    if num % 2 == 0:\n",
    "        even += 1\n",
    "    else:\n",
    "        odd += 1\n",
    "\n",
    "print(\"Even:\", even)\n",
    "print(\"Odd:\", odd)"
   ]
