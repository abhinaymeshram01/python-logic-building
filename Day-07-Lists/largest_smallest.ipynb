    "arr = list(map(int, input(\"Enter elements: \").split()))\n",
    "\n",
    "largest = arr[0]\n",
    "smallest = arr[0]\n",
    "\n",
    "for num in arr:\n",
    "    if num > largest:\n",
    "        largest = num\n",
    "    if num < smallest:\n",
    "        smallest = num\n",
    "\n",
    "print(\"Largest:\", largest)\n",
    "print(\"Smallest:\", smallest)"
