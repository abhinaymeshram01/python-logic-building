   ],
   "source": [
    "s = input(\"Enter string: \")\n",
    "left, right = 0, len(s) - 1\n",
    "\n",
    "while left < right:\n",
    "    if s[left] != s[right]:\n",
    "        print(\"Not Palindrome\")\n",
    "        break\n",
    "    left += 1\n",
    "    right -= 1\n",
    "else:\n",
    "    print(\"Palindrome\")"
   ]
