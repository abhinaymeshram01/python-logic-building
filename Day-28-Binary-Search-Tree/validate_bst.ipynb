   "source": [
    "def is_valid_bst(root, low=float('-inf'), high=float('inf')):\n",
    "    if not root:\n",
    "        return True\n",
    "\n",
    "    if not (low < root.data < high):\n",
    "        return False\n",
    "\n",
    "    return (\n",
    "        is_valid_bst(root.left, low, root.data) and\n",
    "        is_valid_bst(root.right, root.data, high)\n",
    "    )"
   ]
