   "source": [
    "def search_bst(root, key):\n",
    "    if not root or root.data == key:\n",
    "        return root\n",
    "\n",
    "    if key < root.data:\n",
    "        return search_bst(root.left, key)\n",
    "    else:\n",
    "        return search_bst(root.right, key)"
   ]
