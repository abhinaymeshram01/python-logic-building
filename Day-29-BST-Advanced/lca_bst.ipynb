   "source": [
    "def lca_bst(root, p, q):\n",
    "    if not root:\n",
    "        return None\n",
    "\n",
    "    if p < root.data and q < root.data:\n",
    "        return lca_bst(root.left, p, q)\n",
    "\n",
    "    if p > root.data and q > root.data:\n",
    "        return lca_bst(root.right, p, q)\n",
    "\n",
    "    return root"
   ]
