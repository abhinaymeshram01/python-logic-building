   "source": [
    "def list_sum(arr):\n",
    "    if len(arr) == 0:\n",
    "        return 0\n",
    "    return arr[0] + list_sum(arr[1:])\n",
    "\n",
    "\n",
    "arr = list(map(int, input(\"Enter list: \").split()))\n",
    "print(\"Sum:\", list_sum(arr))"
   ]
