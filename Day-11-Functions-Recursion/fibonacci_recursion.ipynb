   "source": [
    "def fib(n):\n",
    "    if n <= 1:\n",
    "        return n\n",
    "    return fib(n - 1) + fib(n - 2)\n",
    "\n",
    "\n",
    "n = int(input(\"Enter terms: \"))\n",
    "for i in range(n):\n",
    "    print(fib(i), end=\" \")"
   ]
