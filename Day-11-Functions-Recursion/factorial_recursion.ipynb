   "source": [
    "def factorial(n):\n",
    "    if n == 0 or n == 1:\n",
    "        return 1\n",
    "    return n * factorial(n - 1)\n",
    "\n",
    "\n",
    "num = int(input(\"Enter number: \"))\n",
    "print(\"Factorial:\", factorial(num))"
   ]
